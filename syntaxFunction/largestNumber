function largest(input1, input2, input3){
    let result = 0;
    if (input1 > input2 && input1 > input3){
         result = input1;
    }else if(input2 > input1 && input2 > input3){
        result = input2;
    }else{
        result = input3;
    }
    console.log(`The largest number is ${result}.`)
}

function solve(numbers){
    let sum = 0;
    
for(let i = 0; i < numbers.length; i++){
    for(let j = 0; j < numbers[i].length; j++){
        if(i + 1 < numbers.length){
        if(numbers[i][j] == numbers[i+1][j]){
            sum++;
        }


    }
    if(j + 1 < numbers[i].length){
        if(numbers[i][j] == numbers[i][j +1]){
            sum++;
        }
    }
    }
}

return sum;
}
